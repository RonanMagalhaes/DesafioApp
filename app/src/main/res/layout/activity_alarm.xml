<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#263238"
    android:clipToPadding="false"
    tools:context=".Alarm">

    <androidx.coordinatorlayout.widget.CoordinatorLayout
        android:id="@+id/coordinatorLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    <com.google.android.material.search.SearchBar
        android:id="@+id/search_bar"
        style="@style/Widget.AppCompat.SearchView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:background="#9DB3BD"
        android:contentDescription=""
        android:hint=""
        app:defaultMarginsEnabled="true"
        app:layout_anchorGravity="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:navigationContentDescription="@string/app_name"
        app:navigationIcon="@drawable/icsearch" />

    <Spinner
        android:id="@+id/spinner4"
        android:layout_width="409dp"
        android:layout_height="50dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="96dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="394dp"
        android:background="#2F3D44"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/icAlarmDevice"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:background="#2F3D44"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/icalarmdevice"
        tools:layout_editor_absoluteY="108dp"
        tools:ignore="MissingConstraints"/>

    <TextView
        android:id="@+id/textView6"
        style="Nunito Sans,SemiBold"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Central de alarme"
        android:textColor="#9DB3BD"
        tools:layout_editor_absoluteX="50dp"
        tools:layout_editor_absoluteY="113dp"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/icKebab"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:background="#2F3D44"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/icoverflow"
        tools:layout_editor_absoluteY="108dp"
        tools:ignore="MissingConstraints"/>

    <include
        android:id="@+id/my_bottom_bar"
        layout="@layout/activity_bottom_bar"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    </androidx.coordinatorlayout.widget.CoordinatorLayout>

</androidx.constraintlayout.widget.ConstraintLayout>